{
  "name": "github-typescript",
  "version": "1.0.0",
  "description": "Build TypeScript scripts and run them via actions/github-script",
  "private": false,
  "keywords": [
    "github-actions",
    "typescript",
    "github-script",
    "composite-action",
    "esbuild"
  ],
  "author": "tkstang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tkstang/github-typescript.git"
  },
  "homepage": "https://github.com/tkstang/github-typescript#readme",
  "bugs": {
    "url": "https://github.com/tkstang/github-typescript/issues"
